{
  "manifest_version": 2,
  "name": "Screen Markup Statistics",
  "description": "Used to give statistics on a web pages markup",
  "version": "0.1",
  "permissions": [
    "tabs", "http://*/*", "https://*/*"
  ],
  "background": {
    "scripts": ["js/vendor/jQuery-2.min.js","js/vendor/jquery.dataTables.js", "js/app.js"]
  },
  "browser_action": {
    "default_icon": "img/image.png"
  }
}